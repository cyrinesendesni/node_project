apiVersion: apps/v1
kind: Deployment
metadata:
  name: node-project
  labels:
    app: nodejs
spec:
  replicas: 1 
  selector:
    matchLabels:
      app: nodejs
  template:
    metadata:
      labels:
        app: nodejs
    spec:
      containers:
        - name: nodejs
          image: cyrinesendesni/node_project:cyrine
          ports:
            - containerPort: 3005
         
